package CYGPKG_IO_SERIAL_ARM_NETX {
    alias             { "Hilscher netX (PL010) serial driver"
                        devs_serial_arm_netx netx_serial_driver }
    hardware
    directory         devs/serial/arm/netx
    script            ser_arm_netx.cdl
    description       "Hilscher netX PL010 serial device driver"
}

package CYGPKG_DEVS_ETH_ARM_NETX500 {
	alias		{ "Hilscher netX500 Standard MAC driver" netx500_eth_driver}
	hardware
	directory	devs/eth/arm/netx500
	script		netx500_eth.cdl
	description	"Ethernet driver for netX500 XC units 0,1."
}

package CYGPKG_HAL_ARM_ARM9_NETX500 {
	alias		{ "Hilscher netX100/500" hal_arm_arm9_netx500 }
